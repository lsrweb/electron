{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,

    "verbatimModuleSyntax":false
  },
  "include": [
    "auto-imports.d.ts",
    "components.d.ts",
    "**/*.d.ts",
    "**/*.ts",
    "src/render_/**/*.vue",
    "src/**/*.ts"
  ]
}
