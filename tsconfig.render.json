{
  "include": [
    "*.d.ts",
    "*.ts",
    "src/render/**/*",
    "src/render/**/*.vue",
    "src/render/**/*.ts",
    "src/render/**/*.tsx",
    "src/render/**/*.jsx",
    "src/render/**/*.d.ts",
    "vite.renderer.config.ts",
    "forge.env.d.ts",
    "vite.base.config.ts",
    "auto-imports.d.ts"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./src/render",
    "paths": {
      "@re/*": ["./*"]
    },
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "sourceMap": true,
    "allowJs": true
  }
}
